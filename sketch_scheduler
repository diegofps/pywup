wup cluster burn --cluster \
    --w '$HOME' \
    --o "./teste_output" \
    --runs 2 \
    --v P1 1 2 3 \
\
    --e TESTE \
    --va P2 1 10 1 \
    --vg P3 1 10 1.5 \
    --c './test.py $P1 $P2 $P3'


wup cluster burn --cluster \
    --w '$HOME/Tracker/dory2/fullapp' \
    --o "./test" \
    --runs 1 \
    --v DS_NAME $DATASETS \
\
    --e WISARD \
    --c "make r6 ds=\$DS_NAME fps=30 extra='-gridSize 15 -cdMaxTrains 30 -cdThreshold 0.8 -cdNumDiscs 10 -model z -ztRamBits 5 -ztReplicate 2 -ztMinConfidence 0'"

